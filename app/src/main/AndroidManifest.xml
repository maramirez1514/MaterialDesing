<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.reidyn.demo.materialdesing">

    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ColoresActivity" />
        <activity android:name=".TipografiaActivity" />
        <activity android:name=".CapasySombras" />
        <activity
            android:name=".PropiedadesActivity"
            android:theme="@style/TemaMaterial" />
        <activity android:name=".CompatibilidadActivity" />
        <activity android:name=".activityCardView" />
        <activity android:name=".botonFlotanteActivity" />
        <activity android:name=".ActivityRecyclerView" />
        <activity android:name=".AnimacionesActivity" />
    </application>

</manifest>